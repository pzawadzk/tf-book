    "    keras.layers.Conv2DTranspose(3, kernel_size=2, strides=2, padding=\"same\",\n",
    "                                 activation=\"sigmoid\"),\n",
    "    keras.layers.Permute([3,1,2]),\n",
    "    keras.layers.Reshape([3072])\n",
    "  \n",
    "])\n",
    "\n"
   ]
